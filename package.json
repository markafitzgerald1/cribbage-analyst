{
    "name": "cribbage-analyst",
    "version": "0.2.3",
    "description": "A cribbage discard and play analysis tool.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/markafitzgerald1/cribbage-analyst.git"
    },
    "scripts": {
        "analyze-jshint": "jshint --config .jshintrc gulpfile.js scripts/ spec/ nightwatchSpec/ src/",
        "analyze-jscs": "jscs --config .jscsrc gulpfile.js scripts/ spec/ nightwatchSpec/ src/",
        "analyze-prettier": "prettier --list-different --config .prettierrc.json gulpfile.js scripts/*.js spec/*.js nightwatchSpec/*.js src/*.js",
        "test-unit": "jasmine spec/*.unitSpec.js",
        "test-integration": "jasmine spec/*.integrationSpec.js",
        "test": "jasmine spec/*.*Spec.js",
        "clean": "rimraf docs dist",
        "jsdoc": "node scripts/jsdoc.js",
        "build": "npm-run-all --parallel --aggregate-output analyze-* test jsdoc",
        "webpack": "webpack src/cribbageAnalyst.js --mode=development --output dist/cribbageAnalyst.bundle.js --output-library cribbageAnalyst",
        "copy": "copyfiles index.* dist",
        "dist": "npm run build && npm-run-all webpack copy",
        "start": "http-server dist"
    },
    "keywords": [
        "cribbage",
        "analysis",
        "analyst",
        "discard",
        "play",
        "cards",
        "crib"
    ],
    "author": "Mark A. Fitzgerald",
    "license": "MPL-2.0",
    "bugs": {
        "url": "https://github.com/markafitzgerald1/cribbage-analyst/issues"
    },
    "homepage": "https://github.com/markafitzgerald1/cribbage-analyst#readme",
    "devDependencies": {
        "chromedriver": "^2.40.0",
        "copyfiles": "^2.1.0",
        "del": "^2.2.0",
        "edgedriver": "^4.17134.0",
        "geckodriver": "^1.11.0",
        "gh-pages": "^1.2.0",
        "gulp": "^3.9.1",
        "gulp-connect": "^3.1.0",
        "gulp-coverage": "^0.3.38",
        "gulp-jasmine": "^2.2.1",
        "gulp-jscs": "^3.0.2",
        "gulp-jsdoc3": "^2.0.0",
        "gulp-jshint": "^2.0.0",
        "gulp-nightwatch": "^0.3.3",
        "gulp-util": "^3.0.7",
        "http-server": "^0.11.1",
        "jasmine": "^3.2.0",
        "js-combinatorics": "^0.5.0",
        "jsdoc": "^3.5.5",
        "jshint": "^2.9.1",
        "lodash": "^4.5.0",
        "npm-run-all": "^4.1.3",
        "prettier": "^1.14.2",
        "selenium-server-standalone-jar": "^3.13.0",
        "vue": "^1.0.16",
        "webpack": "^4.19.1",
        "webpack-cli": "^3.1.1",
        "webpack-stream": "^3.1.0"
    }
}
